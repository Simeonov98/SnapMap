{% extends 'base.html.twig' %}

{% block title %}File{% endblock %}

{% block body %}
    <h1>File</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ file.id }}</td>
            </tr>
            <tr>
                <th>Coord_file</th>
                <td>{{ file.CoordFile }}</td>
            </tr>
            <tr>
                <th>Archive_file</th>
                <td>{{ file.ArchiveFile }}</td>
            </tr>
            <tr>
                <th>Created_at</th>
                <td>{{ file.CreatedAt ? file.CreatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('file_index') }}">back to list</a>

    <a href="{{ path('file_edit', {'id': file.id}) }}">edit</a>

    {{ include('file/_delete_form.html.twig') }}
{% endblock %}
